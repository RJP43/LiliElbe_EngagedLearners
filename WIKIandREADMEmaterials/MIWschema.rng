                  <ref name="lemodel.divGenLike"/>
                  <zeroOrMore>
                     <choice>
                        <ref name="lemodel.global"/>
                        <ref name="lemodel.divGenLike"/>
                     </choice>
                  </zeroOrMore>
               </group>
            </optional>
            <choice>
               <oneOrMore>
                  <group>
                     <ref name="lemodel.divLike"/>
                     <zeroOrMore>
                        <choice>
                           <ref name="lemodel.global"/>
                           <ref name="lemodel.divGenLike"/>
                        </choice>
                     </zeroOrMore>
                  </group>
               </oneOrMore>
               <oneOrMore>
                  <group>
                     <ref name="lemodel.div1Like"/>
                     <zeroOrMore>
                        <choice>
                           <ref name="lemodel.global"/>
                           <ref name="lemodel.divGenLike"/>
                        </choice>
                     </zeroOrMore>
                  </group>
               </oneOrMore>
               <group>
                  <oneOrMore>
                     <group>
                        <ref name="lemodel.common"/>
                        <zeroOrMore>
                           <ref name="lemodel.global"/>
                        </zeroOrMore>
                     </group>
                  </oneOrMore>
                  <optional>
                     <choice>
                        <oneOrMore>
                           <group>
                              <ref name="lemodel.divLike"/>
                              <zeroOrMore>
                                 <choice>
                                    <ref name="lemodel.global"/>
                                    <ref name="lemodel.divGenLike"/>
                                 </choice>
                              </zeroOrMore>
                           </group>
                        </oneOrMore>
                        <oneOrMore>
                           <group>
                              <ref name="lemodel.div1Like"/>
                              <zeroOrMore>
                                 <choice>
                                    <ref name="lemodel.global"/>
                                    <ref name="lemodel.divGenLike"/>
                                 </choice>
                              </zeroOrMore>
                           </group>
                        </oneOrMore>
                     </choice>
                  </optional>
               </group>
            </choice>
            <zeroOrMore>
               <group>
                  <ref name="lemodel.divBottom"/>
                  <zeroOrMore>
                     <ref name="lemodel.global"/>
                  </zeroOrMore>
               </group>
            </zeroOrMore>
         </group>
         <ref name="leatt.global.attributes"/>
         <empty/>
      </element>
