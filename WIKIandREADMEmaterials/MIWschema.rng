                  <ref name="lemodel.divTop"/>
                  <ref name="lemodel.global"/>
               </choice>
            </zeroOrMore>
            <optional>
               <group>
                  <choice>
                     <oneOrMore>
                        <group>
                           <choice>
                              <ref name="lemodel.divLike"/>
                              <ref name="lemodel.divGenLike"/>
                           </choice>
                           <zeroOrMore>
                              <ref name="lemodel.global"/>
                           </zeroOrMore>
                        </group>
                     </oneOrMore>
                     <group>
                        <oneOrMore>
                           <group>
                              <ref name="lemodel.common"/>
                              <zeroOrMore>
                                 <ref name="lemodel.global"/>
                              </zeroOrMore>
                           </group>
                        </oneOrMore>
                        <zeroOrMore>
                           <group>
                              <choice>
                                 <ref name="lemodel.divLike"/>
                                 <ref name="lemodel.divGenLike"/>
                              </choice>
                              <zeroOrMore>
                                 <ref name="lemodel.global"/>
                              </zeroOrMore>
                           </group>
                        </zeroOrMore>
                     </group>
                  </choice>
                  <zeroOrMore>
                     <group>
                        <ref name="lemodel.divBottom"/>
