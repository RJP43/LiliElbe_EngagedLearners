         <oneOrMore>
            <choice>
               <ref name="lemodel.encodingDescPart"/>
               <ref name="lemodel.pLike"/>
            </choice>
         </oneOrMore>
         <ref name="leatt.global.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="leeditorialDecl">
      <element name="editorialDecl">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(editorial
