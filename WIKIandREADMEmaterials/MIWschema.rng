         <ref name="leatt.typed.attributes"/>
         <ref name="leatt.edition.attributes"/>
         <ref name="leatt.spanning.attributes"/>
         <ref name="leatt.breaking.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="leauthor">
      <element name="author">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">in a
